{
  "policy": "warning",
  "patterns": [
    [".*\\d{3}_\\d{4}_\\d{2}\\.mp4$", {
      "filters": {
        "ffprobe.meta": {
          "stream:v:0": {
            "field_mode": 1
          },
          "count:v": 1,
          "count:a": [["gte", 2], ["lte", 4]],
          "format": {
            "bit_rate": ["gte", 20000000]
          }
        }
      }
    }, "ffmpeg.convert", {
      "profile": "x264_itff_422_amix+aac_audio.json"
    }],
    [".*\\.mxf$", {}, "ffmpeg.convert", {
      "profile": "x264_itff_422.json"
    }],
    [".*\\.docx$", {}, "copy", {}],
    [".*\\.(?:xml|smi|ppn|bim|db)", {}, "skip", {}]
  ]
}